<ul class="data-table projected-balance row-highlights" data-test-projected-balance-table>
  <ProjectedBalanceTable::Header
    @month={{this.months}}
  />
  {{#each this.accountsByMonth as |account|}}
    <ProjectedBalanceTable::Row::Account @account={{account}} />
  {{/each}}
  <ProjectedBalanceTable::Row::Totals
    @totalCreditPaymentsByMonth={{this.totalCreditPaymentsByMonth}}
    @overallCreditTotal={{this.overallCreditTotal}}
    @totalsByMonth={{this.totalsByMonth}}
    @overallTotal={{this.overallTotal}}
  />
  <ProjectedBalanceTable::Row::Totals::Differences
    @totalDifferenceByMonth={{this.totalDifferenceByMonth}}
  />
  <ProjectedBalanceTable::Row::Totals::Income
    @monthlyRemainingIncome={{this.monthlyRemainingIncome}}
    @totalRemainingIncome={{this.totalRemainingIncome}}
  />
  <ProjectedBalanceTable::Row::Totals::CreditDebt
    @totalCreditDebtByMonth={{this.totalCreditDebtByMonth}}
  />
  <ProjectedBalanceTable::Row::Totals::Debt
    @totalDebtByMonth={{this.totalDebtByMonth}}
  />
</ul>
